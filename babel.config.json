{
    "presets": [
        ["@babel/preset-env", { "modules": false, "targets": { "node": true } }],
        ["@babel/preset-typescript"]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "babel-plugin-transform-import-meta"
    ]
}